      doc.text(1, 7, "Usuario ATV: " + text);
      text = $("#atvPassword").val();
      doc.text(15, 7, "Contraseña ATV: " + text);
      text = $("#billEmail").val();
      doc.text(1, 8, "Correo de facturas: " + text);
      text = $("#billEmailPassword").val();
      doc.text(15, 8, "Contraseña del correo de facturas: " + text);
      text = $("#billSystemUser").val();
      doc.text(1, 9, "Usuario de facturación: " + text);
      text = $("#billSystemPassword").val();
      doc.text(15, 9, "Contraseña de facturación: " + text);
      text = $("#billSystem").val();
      doc.text(1, 10, "Sistema de facturacion: " + text);
      text = $("#traviUser").val();
      doc.text(1, 11, "Usuario Travi: " + text);
      text = $("#traviPassword").val();
      doc.text(15, 11, "Contraseña Travi: " + text);
      text = $("#traviEmail").val();
      doc.text(1, 12, "Correo Travi: " + text);
