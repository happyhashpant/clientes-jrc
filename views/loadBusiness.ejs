          <div class="col-md-6 form-floating">
            <input
              type="text"
              class="form-control"
              id="ccssPassword"
              name="ccssPassword"
              placeholder="Contraseña CCSS"
              value="<%=business[0].ccssPassword%>"
              readonly
            />
            <label for="ccssPassword">Contraseña CCSS</label>
          </div>
