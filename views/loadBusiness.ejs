    <div class="addBusinessDIV business-grid">
      <!-------------- Account Data Tab ------------------------->
      <div class="contentBackground business-grid-item" id="accountDataPDF">
        <form
          id="generalData"
          class="needs-validation"
          method="post"
          novalidate
        >
          <!------------------------------------------- Edit Buttons General Data ----------------------------------------->
          <div class="editButtons">
            <button
              type="submit"
              class="button hiddenButton"
              id="saveGeneralData"
            >
              <i class="material-icons">sd_card</i>
              <span class="text">Guardar</span>
            </button>
          </div>
          <!------------------------------------------- END Edit Buttons General Data ----------------------------------------->
          <div class="row">
            <div class="col-md-8 form-floating">
              <input
                id="businessName"
                type="text"
                class="form-control"
                name="businessName"
                placeholder="Nombre de la sociedad"
                value="<%=business[0].businessName%>"
                readonly
                required
              />
              <label for="businessName">Nombre</label>
            </div>
            <div class="col-md-4 form-floating">
              <input
                id="businessID"
                type="number"
                class="form-control"
                name="businessID"
                placeholder="Cédula"
                value="<%=business[0].businessID%>"
                readonly
              />
              <label for="businessID">Cédula</label>
            </div>
          </div>
          <div class="row">
            <div class="col-md-2 form-floating">
              <select
                name="businessStatus"
                id="businessStatus"
                class="form-select"
                list="datalistOptions"
                autocomplete="off"
                required
                disabled
              >
                <%if(business[0].statusBusiness == 1){%>
                <option value="1" selected>Activa</option>
                <option value="0">Inactiva</option>
                <%}else{%>
                <option value="1">Activa</option>
                <option value="0" selected>Inactiva</option>
                <% }%>
              </select>
              <label for="businessRegimen">Estado</label>
            </div>
          </div>
        </form>
        <form id="tivDataForm" class="needs-validation" novalidate>
          <input
            name="businessID"
            type="hidden"
            value="<%=business[0].businessID%>"
          />
          <!------------------------------------------- Edit Buttons TIV ----------------------------------------->
          <div class="editButtons">
            <button
              type="submit"
              class="button hiddenButton"
              id="saveTIVDataButton"
            >
              <i class="material-icons">sd_card</i>
              <span class="text">Guardar</span>
            </button>
          </div>
          <!------------------------------------------- END Buttons TIV ----------------------------------------->
          <div class="table-responsive">
            <table class="table table-light table-striped table-bordered tiv">
              <tr>
                <th scope="col">A</th>
                <th scope="col">B</th>
                <th scope="col">C</th>
                <th scope="col">D</th>
                <th scope="col">E</th>
                <th scope="col">F</th>
                <th scope="col">G</th>
                <th scope="col">H</th>
                <th scope="col">I</th>
                <th scope="col">J</th>
                <th scope="col">K</th>
                <th scope="col">L</th>
                <th scope="col">M</th>
                <th scope="col">N</th>
                <th scope="col">O</th>
              </tr>
