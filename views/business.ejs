<!DOCTYPE html>

<html>
  <head>
    <script src="/assets/jquery-3.6.0.min.js"></script>
    <script src="/assets/bootstrap.min.js"></script>
    <script defer src="/assets/functions.js"></script>
    <link rel="stylesheet" href="/assets/bootstrap.min.css" />
    <link rel="stylesheet" href="/assets/styles.css" />
    <link rel="stylesheet" href="/assets/styles.css" />
    <link rel="icon" href="/assets/logo.png" />
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdn.datatables.net/v/bs5/dt-1.11.4/datatables.min.css"
    />
    <script
      type="text/javascript"
      src="https://cdn.datatables.net/v/bs5/dt-1.11.4/datatables.min.js"
    ></script>

    <title>Sociedades</title>
  </head>

  <body>
    <div id="navBar"></div>

    <h1>Sociedades</h1>
    <div class="contentBackground">
      <input
        type="text"
        class="search"
        name="inputSearch"
        id="inputSearch"
        placeholder="Buscar"
        onkeyup="searchTable()"
      />

      <div class="table-responsive">
        <table
          id="table"
          class="table table-light table-striped table-bordered table-hover"
        >
          <thead class="table-dark">
            <tr>
              <th scope="col">Nombre</th>
              <th scope="col">Cedula</th>
              <th scope="col">Sistema De Facturacion</th>
              <th scope="col">Usuario De Facturacion</th>
              <th scope="col">Contrase√±a De Facturacion</th>
            </tr>
          </thead>
          <tbody id="userInfo">
            <% var i=1; objects.forEach(function(data){%>
            <tr
              class="clickable"
              onclick="loadBusiness('<%=data.id%>')"
              onauxclick="loadBusiness2('<%=data.id%>')"
              ;
            >
              <td><%=data.businessName%></td>
              <td><%=data.businessID%></td>
              <td><%=data.billSystem%></td>
              <td><%=data.billSystemUser%></td>
              <td><%=data.billSystemPassword%></td>
            </tr>
            <% i++; }) %>
          </tbody>
        </table>
      </div>
    </div>
  </body>
  <script>
    $(function () {
      $("#navBar").load("/assets/navbar.html");
    });
    $(document).ready(function () {
      $("#table").DataTable({
        
        pageLength: 25
      });
    });
  </script>
</html>
